generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Usuario {
    id        String   @id @default(uuid()) @db.Uuid
    nome      String
    email     String   @unique
    senha     String
    avatar    String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

model Financeiro {
    id        String   @id @default(uuid()) @db.Uuid
    tipo      String
    valor     String
    status    String
    data      String
    descricao String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
